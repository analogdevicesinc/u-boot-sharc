-#
-proc wait_for_uboot {} {
-   global UBOOT_PROMPT
-
-   puts "waiting for uboot prompt"
-   send_log "waiting for uboot prompt"
-   set timeout_flag 0
-   while 1 {
-      expect {
-
-         $UBOOT_PROMPT {
-            break
-         }
-
-         timeout {
-            report "ERROR: Never got uboot prompt, will attempt to recover"
-            set timeout_flag 1
-            # send a Control-C
-            send "\3"
-            break
-         }
-      }
-   }
-   if { $timeout_flag == 1 } {
-      while 1 {
-         expect {
-
-            $UBOOT_PROMPT {
-               report "recovery worked"
-               break
-            }
-
-            timeout {
-               report "ERROR: Wasn't able to recover, will try again"
-               recover
-               break
-            }
-         }
-      }
-   }
