-
-               $UBOOT_PROMPT {
-                  break
-               }
-
-               timeout {
-                  report "ERROR: cmp.b took way too long \
-during cp and cmp test"
-                  recover
-                  break
-               }
-            }
-         }
-         record_test_result $cmp_loc $temp_status_flag
