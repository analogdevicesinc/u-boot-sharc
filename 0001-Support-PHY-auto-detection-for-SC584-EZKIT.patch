-      while 1 {
-         expect {
-            "6" {
-               send "\r"
-               break
-            }
-
-            timeout {
-               report "ERROR: Logic error when timing bootdelay"
-               recover
-               break
-            }
-         }
-      }
-   }]
-   set bootsec [convert_to_seconds $temp_time]
-   record_test_measurement $boot_seconds_loc $bootsec
